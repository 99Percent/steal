if (_args.length < 1) {
	print("USAGE: js steal/generate/model ClassName Type\n")
	print();
	print("    Types = JsonRest")
	print();
	quit();
}

load('steal/rhino/loader.js')
rhinoLoader(function(){
	steal.plugins('view','util/generator','lang/inflector');
});

(function(){
	$modelName = _args[0];
	$modelType = _args[1];
	load("steal/generate/parts/convert.js")
	load("steal/generate/parts/model.js")
	Generator.postGenerationMessage();
})();